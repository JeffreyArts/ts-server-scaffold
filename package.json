{
    "name": "ts-scaffold",
    "version": "1.0.0",
    "description": "A simple TypeScript project with Node.js, Express, and SQLite",
    "main": "server.ts",
    "scripts": {
        "dev": "nodemon --ext ts --exec ts-node src/server.ts",
        "start": "ts-node src/server.ts",
        "populate": "ts-node src/populate.ts",
        "prisma:migrate": "npx prisma migrate dev --name init",
        "prisma:generate": "npx prisma generate"
    },
    "dependencies": {
        "express": "^4.18.2",
        "@prisma/client": "^5.0.0",
        "sqlite3": "^5.1.6"
    },
    "devDependencies": {
        "prisma": "^5.0.0",
        "@faker-js/faker": "^9.5.0",
        "@types/express": "^4.17.13",
        "@types/faker": "^6.6.11",
        "@types/node": "^18.11.9",
        "dotenv": "^16.4.7",
        "nodemon": "^3.1.9",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    }
}
